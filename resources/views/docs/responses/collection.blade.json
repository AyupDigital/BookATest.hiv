{
  "type": "object",
  "properties": {
    "data": {
      "type": "array",
      "items": {!! $data !!}
    },
    "meta": {
      "type": "object",
      "properties": {
        "current_page": {
          "type": "integer",
          "example": 5
        },
        "from": {
          "type": "integer",
          "example": 51
        },
        "last_page": {
          "type": "integer",
          "example": 10
        },
        "path": {
          "type": "string",
          "example": "{{ url('/v1/resource') }}"
        },
        "per_page": {
          "type": "integer",
          "example": 10
        },
        "to": {
          "type": "integer",
          "example": 60
        },
        "total": {
          "type": "integer",
          "example": 100
        }
      }
    },
    "links": {
      "type": "object",
      "properties": {
        "first": {
          "type": "string",
          "example": "{{ url('/v1/resource?page=1') }}"
        },
        "last": {
          "type": "string",
          "example": "{{ url('/v1/resource?page=10') }}"
        },
        "prev": {
          "type": "string",
          "example": "{{ url('/v1/resource?page=4') }}"
        },
        "next": {
          "type": "string",
          "example": "{{ url('/v1/resource?page=6') }}"
        }
      }
    }
  }
}
